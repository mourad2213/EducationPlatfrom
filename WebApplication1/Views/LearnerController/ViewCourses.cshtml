@model IEnumerable<WebApplication1.Models.Course>

<h2>Available Courses</h2>
<ul>
    @foreach (var course in Model)
    {
            <li>
            @course.CourseTitle
                <a href="@Url.Action("EnrollInCourse", "LearnerController", new { courseId = course.CourseId })">Enroll</a>
            </li>
    }
</ul>
